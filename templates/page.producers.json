    "8dd18629-da98-403b-9428-9f3073097fbb": {
      "type": "multi-column",
      "blocks": {
        "template--14395657715787__8dd18629-da98-403b-9428-9f3073097fbb-text_block-1": {
          "type": "text_block",
          "settings": {
            "enable_image": true,
            "image": "shopify:\/\/shop_images\/Qualiance-3.jpg",
            "image_width": 500,
            "title": "Eco-friendly",
            "text": "<p>At their factory, Qualiance harvest rainwater and use solar panels to create part of the energy they use. They are have over 10 certifications to their name!<\/p>",
            "button_label": "",
            "link": ""
          }
        },
        "template--14395657715787__8dd18629-da98-403b-9428-9f3073097fbb-text_block-2": {
          "type": "text_block",
          "settings": {
            "enable_image": true,
            "image": "shopify:\/\/shop_images\/Qualiance-2.jpg",
            "image_width": 500,
            "title": "High Performance",
            "text": "<p>Qualiance are at the cutting edge of performance wear. They heat-seal seam so no water can get through and use advanced garment technology to reduce waste when cutting patterns.<\/p>",
            "button_label": "",
            "link": ""
          }
        },
        "template--14395657715787__8dd18629-da98-403b-9428-9f3073097fbb-text_block-3": {
          "type": "text_block",
