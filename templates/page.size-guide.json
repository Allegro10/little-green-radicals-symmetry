        "3ae1f4ac-76e9-4cf2-b706-84c90c70b702": {
          "type": "html",
          "settings": {
            "width": "three-quarters",
            "code": "<center><p>\n<span><strong>Height:<\/strong><br> Measure from the top of the head to the bottom of their feet when your child is standing up or lying down.<\/span><br>\n<span><strong>Chest:<\/strong><br> Measure around the chest just beneath your child's underarms.<\/span>\n<br> \n<span><strong>Waist:<\/strong><br> Measure the natural waist (the bit where it is curved in) with the child’s abdomen relaxed.<\/span>\n<br> \n<span><strong>Leg length:<\/strong><br> Measure from the waist to bottom of the feet.<\/span>\n<br> \n<span><strong>Arm length:<\/strong><br> Measure from the edge of the shoulder to the wrist when arm is straight. <\/span> \n<\/p><\/center>\n<\/div>"
          }
        }
      },
