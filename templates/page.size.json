        "template--14362388168779__fbf9e44b-c8ce-41d9-bac3-8c8431fd37be-168304145520ced1c1-1": {
          "type": "text",
          "settings": {
            "width": "third",
            "subheading": "",
            "title": "",
            "text": "<p><strong>Height:<br\/><\/strong>Measure from the top of the head to the bottom of their feet when your child is standing up or lying down.<\/p><p><strong>Chest:<br\/><\/strong>Measure around the chest just beneath your child's underarms.<\/p><p><strong>Waist:<\/strong><br\/>Measure the natural waist (the bit where it is curved in) with the child’s abdomen relaxed.<\/p>",
            "button_label": "",
            "button_link": "",
            "button_style": "primary",
            "text_alignment": "left"
          }
        },
        "400434b7-fc61-445f-b1ba-f1dadd83dbbc": {
          "type": "text",
          "settings": {
            "width": "third",
            "subheading": "",
            "title": "",
            "text": "<p><strong>Leg Length:<br\/><\/strong>Measure from the waist to bottom of the feet.<\/p><p><strong>Arm Length:<br\/><\/strong>Measure from the edge of the shoulder to the wrist when arm is straight.<\/p>",
            "button_label": "",
            "button_link": "",
            "button_style": "primary",
            "text_alignment": "left"
          }
        }
      },
